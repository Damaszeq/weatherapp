<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.tomaszwejner.weatherapp.RainChartWindowController">
    <children>
        <LineChart fx:id="rainChart" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                   prefHeight="600" prefWidth="900">
            <xAxis>
                <CategoryAxis label="Data" />
            </xAxis>
            <yAxis>
                <NumberAxis label="Opady (mm)" />
            </yAxis>
        </LineChart>
    </children>
</AnchorPane>
